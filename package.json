{
  "name": "ember-native-root",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-native/ember-native.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "_start:docs": "pnpm --filter docs-app start",
    "build": "pnpm --filter docs-app --filter ember-native build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter docs-app --filter ember-native lint:fix",
    "start": "pnpm build; concurrently 'npm:_start:*' --prefix ' ' --restart-after 5000 --restart-tries -1",
    "test": "turbo run test --output-logs errors-only"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@embroider/vite": "^1.5.0",
    "concurrently": "^9.2.1",
    "ember-cli": "~6.6.0",
    "fast-glob": "^3.3.3",
    "fs-monkey": "^1.1.0",
    "heimdalljs": "^0.2.6",
    "heimdalljs-logger": "^0.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0",
    "release-plan": "^0.17.2",
    "rsvp": "^4.8.5",
    "strip-ansi": "6.0.1",
    "typescript": "^5.9.2",
    "webpack": "^5.104.1"
  },
  "engines": {
    "node": ">= 20.*",
    "npm": "use pnpm",
    "yarn": "use pnpm"
  },
  "pnpm": {
    "overrides": {
      "@embroider/addon-shim": "1.9.0",
      "ember-eslint-parser": "0.5.3",
      "strip-ansi": "6.0.1"
    },
    "patchedDependencies": {
      "@nativescript/unit-test-runner": "patches/@nativescript__unit-test-runner.patch"
    },
    "onlyBuiltDependencies": [
      "@nativescript/core",
      "@nativescript/unit-test-runner",
      "@parcel/watcher",
      "chromedriver",
      "core-js",
      "core-js-pure",
      "esbuild",
      "nativescript",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.23.0",
  "volta": {
    "node": "20.17.0",
    "pnpm": "10.23.0"
  },
  "dependencies": {
    "capture-exit": "^2.0.0",
    "find-up": "^8.0.0",
    "locate-path": "^8.0.0",
    "p-limit": "^7.2.0",
    "p-locate": "^6.0.0",
    "path-exists": "^5.0.0",
    "unicorn-magic": "^0.4.0",
    "yam": "^1.0.0",
    "yocto-queue": "^1.2.2"
  }
}
