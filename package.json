{
  "name": "ember-native-root",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-native/ember-native.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "_start:docs": "pnpm --filter docs-app start",
    "build": "turbo run build --output-logs errors-only",
    "lint": "turbo run _:lint --output-logs errors-only",
    "lint:fix": "turbo run _:lint:fix --output-logs errors-only",
    "start": "pnpm build; concurrently 'npm:_start:*' --prefix ' ' --restart-after 5000 --restart-tries -1",
    "test": "turbo run test --output-logs errors-only"
  },
  "devDependencies": {
    "@glint/core": "^1.5.0",
    "concurrently": "^9.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-ember-template-tag": "^2.0.2",
    "release-plan": "^0.10.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">= 20.*",
    "npm": "use pnpm",
    "yarn": "use pnpm"
  },
  "packageManager": "pnpm@9.12.2",
  "pnpm": {
    "overrides": {
      "kolay": "/Users/patrickpircher/IdeaProjects/ember-native/kolay/kolay-1.2.2.tgz",
      "@universal-ember/kolay-ui": "/Users/patrickpircher/IdeaProjects/ember-native/kolay/ui/universal-ember-kolay-ui-0.0.12.tgz"
    }
  },
  "volta": {
    "node": "20.17.0",
    "pnpm": "9.12.2"
  }
}
