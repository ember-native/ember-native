{
  "name": "ember-native-demo",
  "main": "app/boot.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "devtools": "ember-devtools",
    "dev:ios": "concurrently -k -n vite,ns 'npm run dev:server:ios' 'wait-on tcp:5173 && npm run ios'",
    "dev:android": "concurrently -k -n vite,ns 'npm run dev:server:android' 'wait-on tcp:5173 && npm run android'",
    "dev:server:ios": "VITE_DEBUG_LOGS=1 vite serve -- --env.ios --env.hmr",
    "dev:server:android": "VITE_DEBUG_LOGS=1 vite serve -- --env.android --env.hmr",
    "ios": "VITE_DEBUG_LOGS=1 ns debug ios",
    "android": "VITE_DEBUG_LOGS=1 ns debug android",
    "build": " nativescript build android",
    "debug-test": "pnpm nativescript test android --debug-brk",
    "debug": " nativescript debug android --debug-brk",
    "prepare-android": "npx nativescript prepare android",
    "lint": "concurrently 'pnpm:lint:*(!fix)' --names 'lint:'",
    "lint:fix": "concurrently 'pnpm:lint:*:fix' --names 'fix:'",
    "lint:hbs": "ember-template-lint . --no-error-on-unmatched-pattern",
    "lint:hbs:fix": "ember-template-lint . --fix --no-error-on-unmatched-pattern",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "debug:types": "glint --debug-intermediate-representation"
  },
  "dependencies": {
    "@ember/test-waiters": "^4.1.1",
    "@embroider/macros": "^1.18.1",
    "@nativescript/core": "~8.9.7",
    "@nativescript/theme": "^3.1.0",
    "@nativescript/unit-test-runner": "^4.0.1",
    "@valor/nativescript-websockets": "^2.0.2",
    "chromedriver": "^137.0.1",
    "ember-compatibility-helpers": "^1.2.7",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.0",
    "ember-native": "link:../ember-native",
    "ember-native-devtools": "^1.0.3",
    "ember-source": "^6.6.0",
    "nativescript-ui-listview": "^15.2.3",
    "nativescript-ui-sidedrawer": "^15.2.3",
    "octokit": "^5.0.3",
    "selenium-webdriver": "^4.34.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/eslint-parser": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@ember/test-helpers": "^5.2.2",
    "@embroider/compat": "^4.1.11",
    "@embroider/core": "^4.4.1",
    "@embroider/vite": "^1.4.4",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "^1.5.2",
    "@glint/environment-ember-template-imports": "^1.5.2",
    "@jsdevtools/coverage-istanbul-loader": "3.0.5",
    "@nativescript/android": "^9.0.0",
    "@nativescript/core": "^9.0.7",
    "@nativescript/types": "^9.0.0",
    "@nativescript/vite": "^1.0.4",
    "@rollup/plugin-babel": "^6.1.0",
    "@types/qunit": "2.19.12",
    "babel-import-util": "^3.0.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-ember-template-compilation": "^3.0.0",
    "browserify-fs": "^1.0.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.0",
    "content-tag": "^4.0.0",
    "decorator-transforms": "^2.1.0",
    "ember-cli-version-checker": "^5.1.2",
    "ember-qunit": "^9.0.3",
    "ember-resolver": "^13.1.1",
    "ember-routable-component": "^1.2.0",
    "ember-template-lint": "^7.9.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.3.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^16.5.0",
    "https-browserify": "^1.0.0",
    "karma": "^4.0.0",
    "karma-coverage": "2.2.1",
    "karma-nativescript-launcher": "^1.0.0",
    "karma-qunit": "4.2.1",
    "nativescript": "^9.0.1",
    "nyc": "17.1.0",
    "path-browserify": "^1.0.1",
    "querystring-es3": "^0.2.1",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "socket.io": "^4.8.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vite": "^7.2.7",
    "wait-on": "^9.0.3"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "core-js",
      "@nativescript/core",
      "@nativescript/unit-test-runner",
      "chromedriver",
      "@parcel/watcher",
      "core-js-pure",
      "nativescript",
      "core-js",
      "@nativescript/core",
      "@nativescript/unit-test-runner",
      "chromedriver",
      "@parcel/watcher",
      "core-js-pure",
      "nativescript"
    ],
    "onlyBuiltDependencies": [
      "core-js",
      "@nativescript/core",
      "@nativescript/unit-test-runner",
      "chromedriver",
      "@parcel/watcher",
      "core-js-pure",
      "nativescript"
    ]
  }
}
